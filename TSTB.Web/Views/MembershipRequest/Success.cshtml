@using TSTB.Web
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@inject IHtmlLocalizer<SharedResource> SharedLocalizer
@{
    Layout = "/Views/Shared/_MembershipRequestPartial.cshtml";
    ViewData["Title"] = SharedLocalizer["Success"];
}
<div class="container bg-white sm:pt-12 pt-4 relative">
    <div class="lg:px-24 px-4">
        <h1 class="font-bold text-2xl sm:text-3xl lg:max-w-3xl lg:text-3xl" style="color:darkgreen">
            @SharedLocalizer["You have successfully applied for Membership! We will reach you soon"]
        </h1>
    </div>
</div>

@section Scripts{ }